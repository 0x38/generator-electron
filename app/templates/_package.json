{
  "name": "<%= appName %>",
  "productName": "<%= classifiedAppName %>",
  "version": "0.0.0",
  "description": "My <%= superb %> app",
  "license": "MIT",
  "main": "index.js",
  "repository": "<%= githubUsername %>/<%= appName %>",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . $npm_package_productName --prune --asar --ignore=node_modules/electron-prebuilt --ignore=node_modules/electron-packager --ignore=.git"
  },
  "files": [
    "index.js",
    "index.html",
    "index.css"
  ],
  "keywords": [
    "electron-app"
  ],
  "devDependencies": {
    "electron-packager": "^3.3.0",
    "electron-prebuilt": "^0.25.2"
  }
}
